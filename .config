## Target Device
CONFIG_TARGET_mediatek=y
CONFIG_TARGET_mediatek_filogic=y
CONFIG_TARGET_mediatek_filogic_DEVICE_xiaomi_redmi-router-ax6000-ubootmod=y

## Kernel modules
CONFIG_KERNEL_DEBUG_INFO_BTF=y
CONFIG_KERNEL_DEBUG_INFO=y
CONFIG_KERNEL_DEBUG_INFO_REDUCED=n
CONFIG_KERNEL_BPF_EVENTS=y
CONFIG_KPROBE_EVENTS=y
CONFIG_NET_CLS_ACT=y
CONFIG_NET_SCH_INGRESS=m
CONFIG_NET_INGRESS=y
CONFIG_NET_EGRESS=y
CONFIG_PACKAGE_MAC80211_DEBUGFS=n
CONFIG_PACKAGE_MAC80211_MESH=n

## Luci
CONFIG_PACKAGE_uhttpd=y
CONFIG_PACKAGE_luci-base=y
CONFIG_PACKAGE_luci-app-firewall=y
CONFIG_PACKAGE_luci-mod-admin-full=y
CONFIG_PACKAGE_luci-theme-bootstrap=y

## APP
#CONFIG_PACKAGE_v2ray-core=y
#CONFIG_PACKAGE_v2raya=y
CONFIG_PACKAGE_tailscale=y
CONFIG_PACKAGE_kmod-ikconfig=y
CONFIG_PACKAGE_kmod-sched=y
CONFIG_PACKAGE_tc=y
CONFIG_PACKAGE_iptables-mod-ipopt=y

## remove IPv6
CONFIG_IPV6=n
CONFIG_PACKAGE_ip6tables=n
CONFIG_PACKAGE_odhcp6c=n
CONFIG_PACKAGE_odhcpd-ipv6only=n
CONFIG_PACKAGE_kmod-ipv6=n
CONFIG_PACKAGE_kmod-ip6tables=n
